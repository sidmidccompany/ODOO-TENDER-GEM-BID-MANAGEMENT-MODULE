<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Tender Sequence -->
        <record id="tender_sequence" model="ir.sequence">
            <field name="name">Tender Sequence</field>
            <field name="code">tender.tender</field>
            <field name="prefix">TEN/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- GeM Bid Sequence -->
        <record id="gem_bid_sequence" model="ir.sequence">
            <field name="name">GeM Bid Sequence</field>
            <field name="code">gem.bid</field>
            <field name="prefix">GEM/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
